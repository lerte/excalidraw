{
  "productName": "Excalidraw",
  "version": "0.0.1",
  "identifier": "com.excalidraw.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Excalidraw",
        "height": 720,
        "width": 1280,
        "minWidth": 454
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "copyright": "Copyright Â© 2024 Lerte.",
    "windows": {
      "nsis": {
        "installerIcon": "icons/icon.ico"
      }
    },
    "fileAssociations": [
      {
        "ext": ["excalidraw"]
      },
      {
        "ext": ["excalidrawlib"]
      },
      {
        "ext": ["svg"],
        "mimeType": "image/svg"
      },
      {
        "ext": ["png"],
        "mimeType": "image/png"
      },
      {
        "ext": ["jpg", "jpeg"],
        "mimeType": "image/jpeg"
      }
    ]
  }
}
